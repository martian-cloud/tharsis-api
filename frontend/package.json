    "babel-plugin-relay": "^20.1.1",
    "eslint": "^9.39.2",
    "graphql": "^16.8.1",
    "jsdom": "^26.1.0",
    "relay-compiler": "^20.1.1",
    "vite": "^6.4.1",
    "vite-plugin-checker": "^0.10.1",
