  "overrides": {
    "prismjs": "^1.30.0"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage --watch=false",
    "relay": "relay-compiler --watch",
    "update-schema": "graphql-codegen --config codegen.ts",
    "lint": "eslint --max-warnings=0 ."
  },
  "relay": {
    "src": "./src/",
    "schema": "schema/schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/schema-ast": "^4.0.0",
    "@testing-library/jest-dom": "^6.1.2",
